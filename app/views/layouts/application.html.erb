<!DOCTYPE html>
<html>
<head>
  <title><%= @title %></title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => false %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => false %>
  <%= csrf_meta_tags %>
</head>
<body>

	<div id="container">
		<%= render 'layouts/header' %>
		<%= render 'layouts/navigation' %>
		<%= yield %>
		<%= render 'layouts/footer' %>
	</div>

</body>
</html>
